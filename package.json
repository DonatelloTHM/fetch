{
  "name": "@tkrotoff/fetch",
  "version": "0.3.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/tkrotoff/fetch.git"
  },
  "description": "Fetch wrapper for JSON",
  "license": "MIT",
  "keywords": [
    "fetch",
    "json"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist coverage",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "build:esnext": "tsc --project tsconfig.dist.json --outDir dist --declaration true",
    "build:cjs": "tsc --project tsconfig.dist.json --outDir dist/cjs --module commonjs",
    "build": "npm run clean && npm run build:esnext && npm run build:cjs",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "tsc": "tsc",
    "format": "prettier --write \"**/*\"",
    "lint": "eslint . '**/*.{js,ts}'",
    "precommit": "npm run format && tsc && npm run lint",
    "prepush": "npm run precommit && npm run test",
    "prepublishOnly": "npm run build",
    "publish:beta": "npm version 0.1.0-beta.1 && npm publish --tag next --dry-run"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "fetch-mock": "^7.3.9",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3"
  }
}
