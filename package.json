{
  "name": "@tkrotoff/fetch",
  "version": "0.0.1",
  "description": "Fetch wrapper for JSON",
  "license": "MIT",
  "keywords": [
    "fetch",
    "json"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rm -rf dist coverage stats.json",
    "clean:all": "yarn clean && rm -rf node_modules yarn.lock yarn-error.log",
    "build:esnext": "tsc --noEmit false --outDir dist --declaration true",
    "build:cjs": "tsc --noEmit false --module commonjs --outDir dist/cjs",
    "build": "yarn clean && yarn build:esnext && yarn build:cjs",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prettier": "prettier --write \"**/*\"",
    "eslint": "eslint . '**/*.{js,ts}'",
    "precommit": "tsc && yarn prettier && yarn eslint",
    "prepush": "yarn precommit && yarn test",
    "prepare": "yarn build"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/fetch-mock": "^7.2.3",
    "@types/jest": "^24.0.9",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-jest": "^24.1.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fetch-mock": "^7.3.1",
    "jest": "^24.1.0",
    "node-fetch": "^2.3.0",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3333"
  }
}
