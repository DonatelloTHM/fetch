{
  "name": "@tkrotoff/fetch",
  "version": "0.6.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tkrotoff/fetch.git"
  },
  "description": "Fetch wrapper for JSON",
  "license": "MIT",
  "keywords": [
    "http-client",
    "http",
    "fetch",
    "fetch-api"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist coverage",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "build:esnext": "tsc --project tsconfig.dist.json --outDir dist --declaration true",
    "build:cjs": "tsc --project tsconfig.dist.json --outDir dist/cjs --module commonjs",
    "build": "npm run clean && npm run build:esnext && npm run build:cjs",
    "npm:install:example": "cd example && npm install",
    "build:example": "cd example && ./node_modules/.bin/webpack -d",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "tsc": "tsc",
    "format": "prettier --write \"**/*\"",
    "lint": "npm run tsc && eslint . '**/*.{js,ts}'",
    "precommit": "npm run format && npm run lint",
    "prepush": "npm run test",
    "prepublishOnly": "npm run build",
    "publish:beta": "npm version 0.1.0-beta.1 && npm publish --tag next --dry-run",
    "npm:update": "npm run clean:all && npx npm-check-updates --upgrade && npm install"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/fetch-mock": "^7.3.3",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-jest": "^26.6.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-unicorn": "^23.0.0",
    "fetch-mock": "^9.10.7",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5"
  }
}
